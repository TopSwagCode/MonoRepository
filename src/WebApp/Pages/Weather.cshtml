@page
@model WeatherModel
@{
    ViewData["Title"] = "Weather Forecasts";
}
<h1>@ViewData["Title"]</h1>

<table>
    <thead>
    <tr>
        <th>Date</th>
        <th>Summary</th>
        <th>TemperatureC</th>
        <th>TemperatureF</th>
    </tr>
    </thead>
    <tbody>
@foreach (var forecast in Model.WeatherForecasts)
{
    <tr>
        <td>@nameof(forecast.Date)</td>
        <td>@nameof(forecast.Summary)</td>
        <td>@nameof(forecast.TemperatureC)</td>
        <td>@nameof(forecast.TemperatureF)</td>
    </tr>
}
    </tbody>
</table>



