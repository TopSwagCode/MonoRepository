@page
@model WeatherModel
@{
    ViewData["Title"] = "Weather Forecasts";
}
<h1>@ViewData["Title"]</h1>

<table class="styled-table">
    <thead>
    <tr>
        <th>Date</th>
        <th>Summary</th>
        <th>TemperatureC</th>
        <th>TemperatureF</th>
    </tr>
    </thead>
    <tbody>
@foreach (var forecast in Model.WeatherForecasts)
{
    <tr>
        <td>@forecast.Date</td>
        <td>@forecast.Summary</td>
        <td>@forecast.TemperatureC</td>
        <td>@forecast.TemperatureF</td>
    </tr>
}
    </tbody>
</table>



